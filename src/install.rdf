<?xml version="1.0"?>

<RDF xmlns="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:em="http://www.mozilla.org/2004/em-rdf#">

  <Description about="urn:mozilla:install-manifest">

    <em:name>TestWise Recorder</em:name>
    <em:id>@extension.id@</em:id>
    <em:version>1.4.2</em:version>
    <em:type>2</em:type>
    <em:description>Recording tool to help creating automated test scripts (RWebSpec and Watir) for web applications
    </em:description>
    <em:creator>Zhimin Zhan</em:creator>
    <em:homepageURL>http://www.testwisely.com/</em:homepageURL>
    <em:targetApplication>
      <Description>
        <em:id>{ec8030f7-c20a-464f-9b0e-13a3a9e97384}</em:id>
        <em:minVersion>1.0</em:minVersion>
        <em:maxVersion>8.*</em:maxVersion>
      </Description>
    </em:targetApplication>
    <em:iconURL>chrome://testwise_recorder/content/testwise.png</em:iconURL>
    <!--<em:updateURL>https://testwisely.com/testwise_recorder/update.rdf</em:updateURL>-->
    <em:file>
      <Description about="urn:mozilla:extension:file:testwise_recorder.jar">
        <em:package>content/</em:package>
      </Description>
    </em:file>

  </Description>

</RDF>
